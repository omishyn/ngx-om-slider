{
  "name": "ngx-om-ngx-slider",
  "description": "Angular wrapper library for slider",
  "bugs": "https://github.com/omishyn/ngx-om-slider/issues",
  "version": "14.0.1",
  "license": "MIT",
  "author": {
    "name": "Oleh Mishyn",
    "email": "mishyn.uk@gmail.com",
    "url": "https://github.com/omishyn"
  },
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",

    "deploy": "deploy-to-git",
    "publish": "npm publish ./dist/lib",
    "predeploy": "rimraf ./dist/app && mkdirp ./dist/app",

    "prepare": "npm run prepare:lib && npm run prepare:demo-app",
    "prepare:lib": "node scripts/generate-lib-files.js",
    "prepare:demo-app": "node scripts/generate-demo-app-snippets.js && node scripts/generate-demo-app-docs.js && node scripts/copy-bootstrap-icons.js",
    "start": "npm run prepare && ng serve",
    "build": "npm run build:lib && npm run build:demo-app",
    "build:lib": "npm run prepare && rimraf dist/ngx-slider && ng-packagr -p src/ngx-slider/package.json && scss-bundle -e src/ngx-slider/lib/main.scss -d dist/ngx-slider/scss/ngx-slider.scss",
    "build:demo-app": "npm run prepare && rimraf dist/demo-app && ng build --prod --base-href /ngx-slider/",
    "publish-gh": "scripts/publish-gh-pages.sh"
  },
  "tslint": {
    "extends": "./tslint.json"
  },
  "stylelint": {
    "extends": "./stylelint.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/omishyn/ngx-om-slider.git"
  },
  "private": true,
  "config": {
    "deployToGit": {
      "repository": "git@github.com:omishyn/ngx-om-slider.git",
      "branch": "gh-pages",
      "folder": "dist/app",
      "script": "ng build app --configuration=production --base-href=ngx-om-slider --delete-output-path=false",
      "commit": "Publishing $npm_package_version",
      "user": {
        "name": "Oleh Mishyn",
        "email": "mishyn.ua@gmail.com"
      }
    }
  },
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/cdk": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/flex-layout": "14.0.0-beta.41",
    "@angular/forms": "^14.0.0",
    "@angular/http": "7.2.16",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "@ng-bootstrap/ng-bootstrap": "^2.0.0",
    "bootstrap-icons": "^1.1.0",
    "detect-passive-events": "^2.0.3",
    "rxjs": "^6.6.0",
    "zone.js": "^0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.2.0",
    "@angular-devkit/build-ng-packagr": "^0.1000.0",
    "@angular/cli": "^14.0.0",
    "@angular/compiler-cli": "^14.0.0",
    "@angular/language-service": "^14.0.0",
    "@types/node": "~13.9.1",
    "codelyzer": "^6.0.0",
    "core-js": "^3.6.0",
    "cpx": "^1.5.0",
    "deploy-to-git": "^0.1.0",
    "mkdirp": "^1.0.0",
    "ng-packagr": "~14.1.0",
    "prismjs": "^1.14.0",
    "protractor": "~5.4.0",
    "protractor-console-plugin": "^0.1.1",
    "rimraf": "^3.0.0",
    "scss-bundle": "^2.4.0",
    "stylelint": "^13.6.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "terser": "^5.0.0",
    "ts-node": "^8.10.0",
    "tsickle": "^0.39.0",
    "tslint": "^6.1.0",
    "typedoc": "^0.15.8",
    "typescript": "4.7.4",
    "watch": "^1.0.0",
    "webdriver-manager": "^12.1.7"
  }
}
